.UCenter-bg {
    /* background-image: url(https://image.weilanwl.com/color2.0/index.jpg); */
    background-size: cover;
    height: 450rpx;
    display: flex;
    justify-content: center;
    padding-top: 40rpx;
    overflow: hidden;
    position: relative;
    flex-direction: column;
    align-items: center;
    color: #fff;
    font-weight: 300;
    text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
    opacity: 0.8;
}

.UCenter-bg image {
    width: 200rpx;
    height: 200rpx;
}

.UCenter-bg .gif-wave {
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;
    height: 100rpx;
}

map,
.mapBox {
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;
    height: 100rpx;
}

map,
.mapBox {
    width: 750rpx;
    height: 300rpx;
}

.mgl15 {
    margin-left: 15rpx;
}

.mgb20 {
    margin-bottom: 20rpx;
}

.text-r {
    text-align: right;
}

.mr2 {
    margin-right: 2px;
}

/* 左滑列表操作 */
.mt50 {
    margin-top: 50px;
}

.bb {
    border-bottom: 1px solid #e2e2e2;
}

.new-content {
    width: calc(100% - 120rpx);
    line-height: 1.6em;
}

.new-text-cut {
    min-width: 100% !important;
}

/* 选择器的文字颜色 */
.picker-color {
    color: #6d6d6d !important;
}

/* input 文字大小 */
.cu-form-group input {
    font-size: 28rpx !important;
}

/* 数据显示 */
.no-data {
    width: 100%;
    height: 568px;
    padding-top: 60rpx;
    position: fixed;
    text-align: center;
}

/* 列表样式 */
.title-content {
    padding: 30rpx 0;
    background-color: #ffffff;
}

.title-s {
    border-bottom: 25rpx;
    display: -webkit-box;
    overflow: hidden;
    white-space: normal;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-weight: bolder;
    font-size: medium;
    width: calc(100% - 20rpx);
}

.text-w {
    width: 20rpx;
}

.pd030 {
    padding: 0 30rpx;
}

.pdr0 {
    padding-right: 0;
}

.border-bottom {
    border-bottom: 1px solid #e2e2e2;
}

.w90 {
    width: 90%;
}

.w100 {
    width: 100%;
}

.w10 {
    width: 10%;
}

.mgrl00 {
    margin-right: 0 !important;
    margin-left: 0 !important;
}

.order-item {
    width: 100%;
    display: flex;
    position: relative;
}

/* 人员管理卡片 */
.new-cut-item {
    width: 100% !important;
    left: 30rpx !important;
}

/* 可拖动图标 */
.movable-area-move {
    overflow: hidden;
}

.movable-area-move movable-view {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    background-color: #39b54a;
    color: #ffffff;
}

.mr60 {
    margin-right: 60rpx;
}

.dflx {
    display: flex;
    justify-content: space-between;
}

.mgb15 {
    margin-bottom: 15rpx;
}

.mgt30 {
    margin-top: 30rpx;
}

/* 标题 */
.text-tip-s {
    padding: 20rpx 27rpx;
    display: flex;
    justify-content: space-between;
    color: #39b54a;
}

.bt1 {
    border-bottom: 1px solid #e2e2e2;
}

/* 弹窗中的输入框设置 */
.input-money {
    padding: 20rpx;
}

.input-money input {
    border: 1px solid #e2e2e2;
    border-radius: 5px;
    height: 1.8rem !important;
    min-height: 1.8rem !important;
}

.dis-btn {
    border: 1px solid;
}
